<Defs>
    <RecipeDef>
        <defName>FabricateRawPlasteel</defName>
        <label>make plasteel</label>
        <description>Use advanced manufacturing techniques to create raw plasteel.</description>
        <jobString>Making plasteel.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>20</workAmount>
        <products>
            <Plasteel>1000</Plasteel>
        </products>
        <researchPrerequisite>AdvancedFabrication</researchPrerequisite>
        <skillRequirements>
            <Crafting>8</Crafting>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
        <recipeUsers>
            <li>ElectricSmithy</li>
            <li>ElectricSmelter</li>
        </recipeUsers>
    </RecipeDef>

